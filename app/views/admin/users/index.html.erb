<%=link_to 'Создать пользователя' , new_admin_user_path , class:"btn btn-success mb-2"%>
<table class="table">
  <tr>
    <td>id</td>
    <td>email</td>
    <td>роль</td>
    <td>редактировать</td>
    <td>удалить</td>
  </tr>
<% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%= user.email %></td>
    <td><%= user.role %></td>
    <td><%= link_to 'редактировать' , edit_admin_user_path(user)%></td>
    <td><%= link_to 'x' , admin_user_path(user) , method: :delete%></td>
  </tr>
<% end %>
</table>